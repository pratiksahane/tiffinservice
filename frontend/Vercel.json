{
    "version": 2,
    "builds": [
      {
        "src": "frontend/dist/index.html",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "frontend/dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/frontend/dist/index.html"
      }
    ]
  }
  