{
    "version": 2,
    "builds": [
      {
        "src": "tiffin/src/index.html",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "tiffin/dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/tiffin/dist/$1"
      }
    ]
  }
  